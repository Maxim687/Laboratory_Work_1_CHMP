import math 
x1=56 #точне число sqrt(x1 ) 
x2=23/9 #точне число x2 
x1_1 = 7.48 #наближене число x1 
x2_2 = 2.56 #наближене число x2 
def f (x1, x1_1, x2, x2_2): 
 dx1 = abs((math.sqrt(x1) - x1_1)/ math.sqrt (x1)) 
 dx2 = abs((x2 - x2_2)/x2) 
 if (dx1>dx2): 
 print ("Друга рівність точніше") 
 else: 
 print("Перша рівність точніше") 
f(x1,x1_1,x2,x2_2)
